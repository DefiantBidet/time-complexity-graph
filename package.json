{
  "name": "time-complexity-graph",
  "version": "1.0.0",
  "description": "charts runtime complexity for algorithms",
  "main": "src/ts/index.tsx",
  "scripts": {
    "clean": "rm -rf dist/*",
    "lint": "eslint -c ./lib/eslint/eslintrc.json ./src/ts/*",
    "lint:fix": "eslint -c ./lib/eslint/eslintrc.json ./src/ts/* --fix",
    "lint:quiet": "eslint -c ./lib/eslint/eslintrc.json ./src/ts/* --quiet",
    "start": "npm run clean && webpack-dev-server --mode development --config ./lib/webpack/webpack.config.js",
    "build": "npm run clean && webpack --mode production --config ./lib/webpack/prod.webpack.config.js",
    "test": "jest --config=./lib/jest/jest.config.json",
    "test:watch": "jest --config=./lib/jest/jest.config.json --watchAll"
  },
  "author": "DefiantBidet <James Dean>",
  "license": "ISC",
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "recharts": "2.5.0"
  },
  "devDependencies": {
    "@babel/core": "7.21.8",
    "@babel/preset-env": "7.21.5",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.21.5",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.5.1",
    "@types/node": "20.1.0",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@types/recharts": "1.8.24",
    "@types/webpack": "5.28.1",
    "babel-loader": "9.1.2",
    "babel-plugin-module-resolver": "5.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "8.40.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "html-loader": "4.2.0",
    "html-webpack-plugin": "5.5.1",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "prettier": "2.8.8",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.2",
    "typescript": "5.0.4",
    "webpack": "5.82.0",
    "webpack-cli": "5.0.2",
    "webpack-dev-server": "4.13.3"
  }
}
